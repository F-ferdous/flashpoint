# Copy this file to .env.local and fill in your Firebase project settings
# These are public web keys, safe to expose to the browser, but do NOT commit .env.local

NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# OfferToro credentials (server-side only; do NOT expose Secret Key to client)
OFFERTORO_APP_ID=
OFFERTORO_SECRET_KEY=

# AdGate credentials (server-side only)
ADGATE_API_TOKEN=
ADGATE_S2S_KEY=

# CPX Research credentials (server-side only)
CPX_RESEARCH_APP_ID=
CPX_RESEARCH_SECURE_HASH=
CPX_RESEARCH_SUBID_1=
CPX_RESEARCH_SUBID_2=

# Firebase Admin (server-side only)
# Use a service account or a service-user JSON and set these values.
FIREBASE_ADMIN_PROJECT_ID=
FIREBASE_ADMIN_CLIENT_EMAIL=
# When pasting from JSON, replace newlines with \n
FIREBASE_ADMIN_PRIVATE_KEY=
